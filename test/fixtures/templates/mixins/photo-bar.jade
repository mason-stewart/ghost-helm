mixin photoBar(meta)  
  .container-fluid
    div.image-header(style="background-image: url('" + meta.photo +"'); margin-top: 20px; height: 300px; box-shadow: inset 0 -90px 60px -14px rgba(0,0,0,.65);")
      .row(style="height: 300px;")
        .sub-nav(style="bottom: -130px;")
          .col-md-8.col-md-offset-2.col-xs-12
            h2(style="margin-bottom: 20px; margin-left: 30px;")= meta.title
            .triangle-cutout(style="margin-left: 30px;")